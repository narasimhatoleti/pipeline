pipeline {
  agent any
  tool name: 'M3', type: 'maven'
    stages {
      stage ('Initialization') {
        steps {
        sh '''
          echo "PATH =${PATH}"
          echo "M2_HOME = ${M2_HOME}"
          '''
        }
      }
        stage('Build') {
          steps {
                sh 'mvn clean package'
            }
          }
    }

} 
